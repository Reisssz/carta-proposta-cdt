{
  "extends": [
    "development"
  ],
  "hints": {
    "meta-viewport": "off",
    "compat-api/css": [
      "default",
      {
        "ignore": [
          "backdrop-filter",
          "-webkit-overflow-scrolling",
          "scrollbar-width",
          "scrollbar-width: none"
        ]
      }
    ],
    "axe/name-role-value": [
      "default",
      {
        "link-name": "off"
      }
    ],
    "disown-opener": "off",
    "compat-api/html": [
      "default",
      {
        "ignore": [
          "meta[name=theme-color]"
        ]
      }
    ],
    "no-inline-styles": "off"
  }
}